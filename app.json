{
  "expo": {
    "name": "NewsHub",
    "slug": "NewsHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "39.0.0",
    "android": {
      "package": "com.philane.newshub",
      "versionCode": 1
    },
    "icon": "./assets/ic_launcher.png",
    "splash": {
      "image": "./assets/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
